@article{MANGANO2002343,
title = {Multijet matrix elements and shower evolution in hadronic collisions: Wbb̄+n-jets as a case study},
journal = {Nuclear Physics B},
volume = {632},
number = {1},
pages = {343-362},
year = {2002},
issn = {0550-3213},
doi = {https://doi.org/10.1016/S0550-3213(02)00249-3},
url = {https://www.sciencedirect.com/science/article/pii/S0550321302002493},
author = {Michelangelo L. Mangano and Mauro Moretti and Roberto Pittau},
abstract = {We study in this paper the production, in hadronic collisions, of final states with W gauge bosons, heavy quark pairs and n extra jets (with n up to 4). The complete partonic tree-level QCD matrix elements are evaluated using the ALPHA algorithm, and the events generated at the parton level are then evolved through the QCD shower and eventually hadronised using the coherent shower evolution provided by the HERWIG Monte Carlo. We discuss the details of our Monte Carlo implementation, and present results of phenomenological interest for the Tevatron Collider and for the LHC. We also comment on the impact of our calculation on the backgrounds to W(H→bb̄) production, when only one b-jet is tagged.}
}

